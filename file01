pipeline {
    agent any
    stages {
        stage ("build") {
            steps{
                echo "hello world"
            }
        }
        
        stage (quality){
            steps{
                echo "hello hsc"
            }
        }
        stage (testing){
            steps{
                echo "testing code"
            }
        }
        stage (deploy){
            steps{
                echo "deploy the code"
            }
        }    
        
            
    }
    
}    
